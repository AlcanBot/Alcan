{
    "compilerOptions": {
   
      "target": "es2020",                           
      "module": "commonjs",                           
      
      "allowJs": true,                   
  
      "outDir": "./dist",                               
      "moduleResolution": "node",                 
      "baseUrl": "./src",               
       "paths": {
         "@classes/*": ["classes/*"],
         "@handlers/*": ["classes/handlers/*"],
         "@util/*": ["util/*"],
         "@dash/*": ["dashboard/*"],
         "@dash/oauth/*": ["dashboard/oauth/*"],
         "@dash/db/*": ["dashboard/db/*"]
       },       
      "allowSyntheticDefaultImports": true,       
      "esModuleInterop": true,                     
      "skipLibCheck": true,                        
      "forceConsistentCasingInFileNames": true       
    }
  }